
public class TesteSistema {
	
	private int senha = 2222;

	public void autentica(Gerente g) {
		boolean autenticou = g.autentica(this.senha);
		if(autenticou) {
			System.out.println("Acesso Liberado ao Sistema, bem Vindo ");
		} else {
			System.out.println("Acesso NÃ£o liberado, senha invalida");
		}
	}
}

